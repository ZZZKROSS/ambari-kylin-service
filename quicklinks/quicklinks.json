{
  "name": "default",
  "description": "default quick links configuration",
  "configuration": {

    "links": [
      {
        "name": "kylin_ui",
        "label": "KYLIN UI",
        "requires_user_name": "false",
        "component_name": "NGINX_SERVER",
        "url":"%@://%@:%@/kylin",
        "port":{
          "http_property": "nginx_port",
          "http_default_port": "80",
          "https_property": "nginx_port",
          "https_default_port": "80",
          "regex": "^(\\d+)$",
          "site": "nginx"
        }
      }
    ]
  }
}
